{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "$lib/*": [
        "src/lib/*"
      ],
      "$components/*": [
        "src/components/*"
      ],
      "$layouts/*": [
        "src/layouts/*"
      ],
      "$pages/*": [
        "src/pages/*"
      ],
      "$styles/*": [
        "src/styles/*"
      ],
      "$assets/*": [
        "src/assets/*"
      ],
      "$examples/*": [
        "src/examples/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "exclude": [
    "public/pagefind",
    "dist"
  ]
}